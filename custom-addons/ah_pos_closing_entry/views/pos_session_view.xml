<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <record id="ah_pos_session_view" model="ir.ui.view">
            <field name="name">ah.pos.session.view</field>
            <field name="model">pos.session</field>
            <field name="inherit_id" ref="point_of_sale.view_pos_session_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='cash_register_balance_start']" position="after">
                    <field name="pos_attachment_name" invisible="1"/>
                    <field name="pos_attachment_id" filename="pos_attachment_name"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
