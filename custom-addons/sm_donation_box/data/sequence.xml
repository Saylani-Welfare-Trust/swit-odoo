<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="donation_box_registration_sequence" model="ir.sequence">
            <field name="name">Donation Box Registration</field>
            <field name="code">donation_box_registration</field>
            <field name="prefix">DBR/%(year)s/</field>
            <field name="padding">5</field>
        </record>
        
        <record id="donation_box_lock_sequence" model="ir.sequence">
            <field name="name">Donation Box Lock</field>
            <field name="code">donation.box.lock</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
        </record>
        
        <record id="donation_box_key_sequence" model="ir.sequence">
            <field name="name">Donation Box Key</field>
            <field name="code">donation.box.key</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
        </record>
    
    </data>
    

</odoo>
