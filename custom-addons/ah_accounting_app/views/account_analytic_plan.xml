<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="ah_account_analytic_plan_view_form" model="ir.ui.view">
            <field name="name">ah.account.analytic.plan.view.form</field>
            <field name="model">account.analytic.plan</field>
            <field name="inherit_id" ref="analytic.account_analytic_plan_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='business_domain']" position="after">
                    <field name="company_id"/>
                    <field name="account_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>