<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock.view_production_lot_form_inherit_bn_donation_box" model="ir.ui.view">
        <field name="name">view.production.lot.form.inherit.bn.donation.box</field>
        <field name="model">stock.lot</field>
        <field name="inherit_id" ref="stock.view_production_lot_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='ref']" position="after">
                <field name="lot_consume" readonly="1" />
            </xpath>
        </field>
    </record>

</odoo>
