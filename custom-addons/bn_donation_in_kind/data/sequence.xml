<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="donation_in_kind_sequence" model="ir.sequence">
            <field name="name">Donation In Kind</field>
            <field name="code">donation_in_kind</field>
            <field name="prefix">DIK/%(year)s/</field>
            <field name="padding">5</field>
        </record>
    
        <record id="donation_in_kind_line_sequence" model="ir.sequence">
            <field name="name">Donation In Kind Line</field>
            <field name="code">donation_in_kind_line</field>
            <field name="prefix">DIKL/%(year)s/</field>
            <field name="padding">5</field>
        </record>
        
        <record id="product_valuation_committee_line_sequence" model="ir.sequence">
            <field name="name">Product Valuation Committee Line</field>
            <field name="code">product_valuation_committee_line</field>
            <field name="prefix">PVCL/%(year)s/</field>
            <field name="padding">5</field>
        </record>
    
    </data>
    

</odoo>
