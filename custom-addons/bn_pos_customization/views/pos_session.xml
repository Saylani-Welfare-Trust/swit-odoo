<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="point_of_sale_view_pos_session_tree_inherit_bn_pos_customization" model="ir.ui.view">
        <field name="name">point.of.sale.view.pos.session.tree.inherit.bn.pos.customization</field>
        <field name="model">pos.session</field>
        <field name="inherit_id" ref="point_of_sale.view_pos_session_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='user_id']" position="after">
                <field name="analytic_account_id" string="Branch" options="{'no_create': True, 'no_edit': True, 'no_open': True}" />
            </xpath>
        </field>
    </record>

</odoo>
