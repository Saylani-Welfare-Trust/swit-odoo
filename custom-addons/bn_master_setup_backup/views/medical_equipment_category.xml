<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="bn_master_setup_medical_equipment_category_view_tree_inherit_bn_master_setup_backup" model="ir.ui.view">
        <field name="name">bn.master.setup.medical.equipment.category.view.tree.inherit.bn.master.setup.backup</field>
        <field name="model">medical.equipment.category</field>
        <field name="inherit_id" ref="bn_master_setup.medical_equipment_category_view_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="attributes">
                <attribute name="domain">[('is_medical_equipment', '=', True)]</attribute>
            </xpath>
        </field>
    </record>

</odoo>
