<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="edit_donor_rule" model="ir.rule">
        <field name="name">Edit Donor</field>
        <field name="model_id" ref="base.model_res_partner"/>
        <field name="domain_force">['|', ('category_id', '=', False), ('category_id.name', 'in', ['Donor'])]</field>
        <field name="groups" eval="[(4, ref('bn_profile_management.profile_management_edit_donor_access_group'))]"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_create" eval="False"/>
        <field name="perm_unlink" eval="False"/>
    </record>

</odoo>
