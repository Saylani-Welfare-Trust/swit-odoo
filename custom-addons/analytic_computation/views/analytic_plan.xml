<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_analytic_plan_form_view_inherit_computation" model="ir.ui.view">
        <field name="name">account.analytic.plan.form.inherit</field>
        <field name="model">account.analytic.plan</field>
        <field name="inherit_id" ref="analytic.account_analytic_plan_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='color']" position="after">
                <field name="credit"/>
                <field name="debit"/>
                <field name="balance"/>
            </xpath>
        </field>
    </record>

</odoo>
