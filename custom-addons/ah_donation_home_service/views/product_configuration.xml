<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="ah_dhs_product_conf_view_tree" model="ir.ui.view">
            <field name="name">ah.dhs.product.conf.view.tree</field>
            <field name="model">dhs.product.conf</field>
            <field name="arch" type="xml">
                <tree editable="bottom" default_order="create_date asc">
                    <field name="product_id" required="1"/>
                    <field name="return_product_id" required="1"/>
                </tree>
            </field>
        </record>


        <record id="action_dhs_product_configuration" model="ir.actions.act_window">
            <field name="name">Product Configuration</field>
            <field name="res_model">dhs.product.conf</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>

