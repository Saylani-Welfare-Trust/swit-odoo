<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="donation_sequence" model="ir.sequence">
        <field name="name">Donation Sequence</field>
        <field name="code">donation</field>
        <field name="prefix">DN/%(year)s/%(month)s/%(day)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <record id="fee_box_sequence" model="ir.sequence">
        <field name="name">Fee Box Sequence</field>
        <field name="code">fee_box</field>
        <field name="prefix">FB/%(year)s/%(month)s/%(day)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
